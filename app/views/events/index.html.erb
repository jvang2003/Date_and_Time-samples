<% title "Events" %>

<table>
  <tr>
    <th>Name</th>
    <th>Start At</th>
    <th>End At</th>
    <th>All Day</th>
  </tr>
  <% for event in @events %>
    <tr>
      <td><%= event.name %></td>
      <td><%= event.start_at %></td>
      <td><%= event.end_at %></td>
      <td><%= event.all_day %></td>
      <td><%= link_to "Show", event %></td>
      <td><%= link_to "Edit", edit_event_path(event) %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Event", new_event_path %></p>
